{
  "name": "@v6y/bfb-main-analyzer",
  "version": "0.0.0",
  "type": "module",
  "main": "./api/index.js",
  "files": [
    "api",
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ekino/v6y"
  },
  "scripts": {
    "start:dev": "nodemon -L api/index.js -- --dev",
    "start": "pm2 start ecosystem.config.cjs",
    "stop": "pm2 stop v6y-bfb-main-analyzer",
    "logs": "pm2 logs",
    "info": "pm2 list",
    "monitor": "pm2 monit",
    "build": "echo '--BUILD BFF--' ",
    "code:analyze": "es6-plato -r -d target/report api",
    "lint": "eslint --config ./.eslintrc.cjs --ignore-path ./.eslintignore --cache-location ./.eslintcache \"./**/*.js\" --cache",
    "lint:fix": "eslint --config ./.eslintrc.cjs --ignore-path ./.eslintignore --cache-location ./.eslintcache \"./**/*.js\" --cache --fix",
    "test:dev": "jest --config=jest.config.json --all --coverage",
    "generate:heapdump": "node ./HeapDump.js"
  },
  "devDependencies": {
    "es6-plato": "=1.2.3",
    "nodemon": "=2.0.19",
    "pm2": "=5.3.0",
    "@babel/preset-env": "=7.23.6",
    "babel-jest": "=29.7.0",
    "chai": "=4.3.10",
    "eslint": "=7.5.0 || =8.0.0 || =9.0.0",
    "jest": "=29.7.0",
    "jest-sonar-reporter": "=2.0.0",
    "jest-stare": "=2.5.1"
  },
  "dependencies": {
    "@v6y/commons": "workspace:^",
    "body-parser": "=1.20.1",
    "compare-versions": "=5.0.1",
    "cookie-parser": "=1.4.6",
    "cors": "=2.8.5",
    "cron": "=2.2.0",
    "express": "=4.18.2",
    "express-status-monitor": "=1.3.4",
    "mongoose": "=7.1.0",
    "axios": "=1.6.2",
    "axios-retry": "=4.0.0"
  },
  "jest-junit": {
    "output": "target/jest-sonar/test-report.junit.xml"
  }
}
